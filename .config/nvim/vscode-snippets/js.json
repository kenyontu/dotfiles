{
  "console.log": {
    "prefix": "cl",
    "body": [
      "console.log($0)"
    ]
  },
  "import": {
    "prefix": "im",
    "body": [
      "import $2 from '$1'"
    ]
  },
  "import named export": {
    "prefix": "imn",
    "body": [
      "import {$2} from '$1'"
    ]
  },
  "require": {
    "prefix": "req",
    "body": [
      "const $2 = require('$1')"
    ]
  },
  "getElementById": {
    "prefix": "gid",
    "body": [
      "const $0 = document.getElementById(\"$1\")"
    ]
  },
  "createElement": {
    "prefix": "cre",
    "body": [
      "const $0 = document.createElement(\"$1\")"
    ]
  },
  "tryCatch": {
    "prefix": "tr",
    "body": [
      "try {",
      "$0",
      "} catch(error) {",
      "",
      "}"
    ]
  },
  "express app setup": {
    "prefix": "express",
    "body": [
      "import express from 'express'",
      "import cors from 'cors'",
      "",
      "const app = express()",
      "",
      "app.use(cors)",
      "app.use(express.json())",
      "app.use(express.urlencoded({ extended: true }))",
      "",
      "const PORT = process.env.PORT || 3000",
      "app.listen(PORT, () => console.info(`Server started at port ${PORT}`))"
    ]
  }
}
