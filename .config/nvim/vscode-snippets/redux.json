{
  "Redux starter kit slice": {
    "prefix": "rslice",
    "body": [
      "import { createSlice, PayloadAction } from 'redux-starter-kit'",
      "",
      "interface $1State {",
      "  $2",
      "}",
      "",
      "const initialState: $1State = {",
      "  $3",
      "}",
      "",
      "const ${1/(.)/${1:/downcase}/}Slice = createSlice({",
      "  slice: '${1/(.)/${1:/downcase}/}',",
      "  initialState,",
      "  reducers: {",
      "    $0",
      "  }",
      "})",
      "",
      "export const {} = ${1/(.)/${1:/downcase}/}Slice.actions",
      "",
      "export default ${1/(.)/${1:/downcase}/}Slice.reducer"
    ]
  },
  "Redux toolkit slice reducer": {
    "prefix": "sred",
    "body": [
      "$1(state, action: PayloadAction<$2>) {",
      "  $0",
      "}"
    ]
  }
}
